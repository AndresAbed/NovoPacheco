   <div id="portfoliowrap" class="marcas">
    <h1 class="teventos">NUESTRAS MARCAS</h1>
    <p class="bajadamarcas">Si tenés una marca y querés sumarte a Novo Pacheco, consultá para tener una reunión</p>
    <button class="btn btn-lg btn-primary" href="https://docs.google.com/forms/d/1cs6XtZXC6RCvUYh9YN0AcSuMXcO7kcT1xWTLFRHIcOY/viewform?usp=send_form" role="button" target="_blank">Consultar aquí</button>

    <div class="portfolio-centered">
      <div class="recentitems portfolio">
        <% @brands.each do |b| %>
          <div class="portfolio-item graphic-design">
            <div class="he-wrap tpl6">
              <img src="<%= b.logo.url %>" alt="">
              <div class="he-view">
                <div class="bg a0" data-animate="fadeIn">
                  <h3 class="a1" data-animate="fadeInDown">Conocé el local</h3>
                  <a data-rel="prettyPhoto" href="<%= b.logo.url %>"></a>
                  <%= link_to 'Ver más', brand_path(b), class: 'dmbutton a2', "data-animate" => "fadeInUp"%>
                </div><!-- he bg -->
              </div><!-- he view -->    
            </div><!-- he wrap -->
          </div><!-- end col-12 -->
        <% end %>
      </div><!-- portfolio -->
    </div><!-- portfolio container -->
   </div><!--/Portfoliowrap -->